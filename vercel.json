{
  "functions": {
    "app.py": {
      "runtime": "python@3.9"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api|assets|favicon.ico|robots.txt|sitemap.xml|.*\\.).*)",
      "destination": "/app.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}